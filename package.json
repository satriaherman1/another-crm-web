{
  "name": "another-crm",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "node server",
    "build:client": "tsc && vite build --outDir dist/client",
    "build:server": "tsc && vite build --outDir dist/server --ssr src/server/entry-server.js ",
