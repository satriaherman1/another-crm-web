    "generate": "vite build --outDir dist/ && npm run build:server && node prerender"
  },
  "dependencies": {
    "express": "^4.17.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-otp-input": "^2.4.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "sass": "^1.50.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-windicss": "^1.8.4",
    "windicss": "^3.5.1"
  }
}
